{
    "version": 2,
    "cleanUrls": true,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "continue": true
        }
    ],
    "headers": [
        {
          "source": "/contact",
          "headers" : [
            {
              "key" : "Cache-Control",
              "value" : "no-cache no-store must-revalidate"
            }
          ]
        },    
  }