{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "continue": true
        },
        {
            "src": "/contact",
            "headers": {
              "key" : "Cache-Control",
              "value" : "no-cache no-store must-revalidate"
            }
        }
    ]
}
